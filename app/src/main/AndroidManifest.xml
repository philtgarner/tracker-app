<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tracker.garner.com.locationtracker" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".AddTracker"
            android:label="@string/app_name"
            android:parentActivityName=".TrackerList">
        </activity>
        <activity
            android:name=".Tracker"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".LaunchTracker"
            android:label="@string/app_name"
            android:parentActivityName=".TrackerList" >
        </activity>
        <activity
            android:name=".Settings"
            android:label="@string/app_name"
            android:parentActivityName=".TrackerList" >
        </activity>
        <activity
            android:name=".PrivacyPicker"
            android:label="@string/app_name"
            android:parentActivityName=".Settings" >
        </activity>
        <activity
            android:name=".TrackerList"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".TrackingService" />

        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyAyGbRV7R-QKqRumYvtwZHmi8d9oi9KZU0" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="4132500" />
    </application>

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>


</manifest>
